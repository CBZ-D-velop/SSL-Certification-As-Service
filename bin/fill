#!/bin/bash

# Add a file in all ssl folder, to be gitted
for ssl_dir in ./../ssl/*; do
    if [ -d "$ssl_dir" ]; then
        for sub_ssl_fir in "$ssl_dir"/*; do
            if [ -d "$sub_ssl_fir" ]; then
                echo "fill: Added in $sub_ssl_fir/empty.lock"
                echo "JUST FOR GIT" > "$sub_ssl_fir/empty.lock"
            fi
        done
    fi
done
